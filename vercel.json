{
    "version": 2,
    "routes": [
      {
        "src": "/sw.js",
        "continue": true,
        "headers": {
          "Cache-Control": "public, max-age=0, must-revalidate",
          "Service-Worker-Allowed": "/"
        }
      }
    ],
    "functions": {
      "api/**/*.js": {
        "excludeFiles": "api/routes/index.js"
      }
    },
    "builds": [
      {
        "src": "nuxt.config.js",
        "use": "@nuxtjs/vercel-builder"
      }
    ]
  }